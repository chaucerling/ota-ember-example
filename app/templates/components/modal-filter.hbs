{{yield}}
<div class="modal-body-title">
  <label>品牌选择</label>
  <label class="modal-finish" {{action 'loadhotel' }}>完成</label>
</div>
<div class="select-content">
  {{#each items as item}}
    <div {{action 'select' filter item.value on="click"}}>
      <label>{{item.title}}</label>
      <img class="selected-image" src="assets/images/selected.png">
    </div>
  {{/each}}
</div>
